{
  "manifest_version": 3,
  "name": "Common Resolve",
  "version": "1.0",
  "description": "Capture Ghost Job on LinkedIn.",
  "icons": {
    "16": "/icons/icon.png",
    "48": "/icons/icon.png",
    "128": "/icons/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon.png",
      "48": "/icons/icon.png",
      "128": "/icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/jobs/*"],
      "js": ["content.js"]
    }
  ]
}
